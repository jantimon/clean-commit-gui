{
  "presets": ["es2015", "stage-0", "react"],
  "plugins": [
    "add-module-exports",
    "syntax-async-functions",
    "transform-regenerator",
    "babel-plugin-transform-es2015-destructuring"
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "test": {
      "plugins": [
        ["webpack-loaders", { "config": "webpack.config.node.js", "verbose": false }]
      ]
    }
  }
}
